{
    "version": 2,
    "builds": [
        {
            "src": "/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js",
            "methods":["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers":{
                "Access-Control-Allow-Origin": "*"
            }
        }
    ],
      "redirects":[
      {
         "source":"/",
         "destination":"https://swole-society-api.vercel.app/"
      }
   ]
}
