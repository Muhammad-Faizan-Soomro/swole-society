{
    "version": 2,
    "builds": [
        {
            "src": "/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/product/top",
            "dest": "https://swole-society-api.vercel.app/api/product/top",
            "methods":["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers":{
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}
